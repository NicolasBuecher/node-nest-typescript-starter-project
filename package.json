{
  "name": "nest-typescript-starter-project",
  "version": "1.0.0",
  "description": "Node + Nest + TypeScript starter project",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "npm run prebuild && tsc --project tsconfig.build.json",
    "start": "npm run build && node dist/main.js",
    "start:dev": "npx nodemon",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --passWithNoTests --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasBuecher/node-nest-typescript-starter-project.git"
  },
  "keywords": [
    "node",
    "nestjs",
    "typescript",
    "starter"
  ],
  "author": "Nicolas Buecher",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NicolasBuecher/node-nest-typescript-starter-project/issues"
  },
  "homepage": "https://github.com/NicolasBuecher/node-nest-typescript-starter-project#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-angular": "^17.3.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
